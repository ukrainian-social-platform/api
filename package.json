{
	"name": "usp-api-service",
	"version": "0.0.1",
	"license": "BSD-3-Clause",
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "nest build && node build && cd dist && rm -rf src tsconfig.build.tsbuildinfo && yarn install --frozen-lockfile && rm yarn.lock package.json",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "set -o allexport && source ./.env && set +o allexport && cd dist && NODE_ENV=production node main",
		"lint": "eslint \"src/**/*.ts\" --fix"
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.6.10",
		"@grpc/proto-loader": "^0.7.2",
		"@nestjs/common": "^9.0.11",
		"@nestjs/config": "^2.2.0",
		"@nestjs/core": "^9.0.11",
		"@nestjs/microservices": "^9.0.11",
		"amqp-connection-manager": "^4.1.3",
		"amqplib": "^0.10.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"pg": "^8.7.3",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.5.6"
	},
	"devDependencies": {
		"@nestjs/cli": "^9.0.0",
		"@nestjs/schematics": "^9.0.1",
		"@types/node": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"@yarnpkg/lockfile": "^1.1.0",
		"esbuild": "^0.14.43",
		"eslint": "^8.22.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.7.1",
		"source-map-support": "^0.5.21",
		"typescript": "^4.3.5"
	}
}
